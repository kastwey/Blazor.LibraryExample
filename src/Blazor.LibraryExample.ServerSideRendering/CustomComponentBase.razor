@code
{

	[Parameter] public CustomPage RootPage { get; set; }

	protected override async Task OnAfterRenderAsync(bool firstRender)
	{
		await base.OnAfterRenderAsync(firstRender);
		if (RootPage != null && !String.IsNullOrWhiteSpace(RootPage.QuerySelectorToFocusAfterRendering))
		{
			await RootPage.DoDefaultFocus();
		}
	}
}